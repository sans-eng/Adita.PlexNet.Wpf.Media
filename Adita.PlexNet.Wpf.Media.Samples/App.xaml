<Application x:Class="Adita.PlexNet.Wpf.Media.Samples.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Adita.PlexNet.Wpf.Media.Samples"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="FontAwesome">pack://application:,,,/Adita.PlexNet.Wpf.Media.Samples;component/Resources/Fonts/#Font Awesome 5 Free Solid</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Adita.PlexNet.Wpf.Media.Samples;component/Resources/Fonts/GlyphNames.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
